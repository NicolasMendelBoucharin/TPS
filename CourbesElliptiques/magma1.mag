//g)
discrim :=function( a1, a2, a3, a4, a6)
    b2:=a1^2 + 4*a2;
    b4:= 2*a4 +a1*a3;
    b6:= a3^2 + 4*a6;
    b8 := a1^2*a6+4*a2*a6-a1*a3*a4+a2*a3^2-a4^2;
    dis:=-b2^2*b8-8*b4^3-27*b6^2 + 9*b2*b4*b6;
    return dis;
end function;

