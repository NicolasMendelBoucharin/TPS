//c)
jinv :=function( a1, a2, a3, a4, a6)
    b2:=a1^2 + 4*a2;
    b4:= 2*a4 +a1*a3;
    dis := discrim( a1, a2, a3, a4, a6);
    c4:=b2^2-24*b4;
    j:=c4^3/dis;
    return j;
end function;
